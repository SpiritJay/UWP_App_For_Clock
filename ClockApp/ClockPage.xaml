                <Button x:Name="commonColorPickerConfirm" Grid.Column="0" HorizontalAlignment="Stretch" Content="确认"
                        Click="CommonColorPickerConfirm_Click"/>
                <Button x:Name="commonColorPickerCancel" Grid.Column="2" HorizontalAlignment="Stretch" Content="取消"
                        Click="CommonColorPickerCancel_Click"/>
            </Grid>
        </StackPanel>

        <StackPanel x:Name="fullScreenTipStackPanel" Orientation="Horizontal" CornerRadius="12" Opacity="0"
                    HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,20,0,0">
            <StackPanel.Background>
                <AcrylicBrush BackgroundSource="Backdrop" FallbackColor="White" TintOpacity="0.2" TintColor="White" TintLuminosityOpacity="0.5"/>
            </StackPanel.Background>
            <Border BorderBrush="Black" BorderThickness="4" CornerRadius="6" Margin="20">
                <TextBlock Text="esc" FontStyle="Italic" FontWeight="Bold" FontSize="20" Margin="10,0,10,5"/>
            </Border>
            <TextBlock Text="退出全屏" FontWeight="Bold" FontSize="20" Margin="0,25,20,25"/>
        </StackPanel>
    </Grid>
</Page>
